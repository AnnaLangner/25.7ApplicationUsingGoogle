doctype html
html
    head
        meta(charset='UTF-8')
        title log in with Google
        link(
            rel='stylesheet',
            href='../google.css',
            type='text/css'
                    )
    body
        .logo
            img(
                src='../google.png'
                alt='google'
            )
        form(action='http://localhost:3000/auth/google', method='GET')
            p
                | User Name:
                input(type='text', placeholder='user_name')
            P
                | Password:
                input(type='password', placeholder='password')
            input(type='submit', value='Submit')